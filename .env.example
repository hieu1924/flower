# Google Sheets API Configuration
# Get this URL after deploying Google Apps Script as Web App
VITE_GOOGLE_SHEETS_API_URL=https://script.google.com/macros/s/YOUR_DEPLOYMENT_ID/exec

# Google Spreadsheet ID (from the URL: docs.google.com/spreadsheets/d/SPREADSHEET_ID/edit)
VITE_GOOGLE_SPREADSHEET_ID=1xFB9YFerq7JXSNCYFI2UrhCgkbOomYIR49kwwgCQkEk

# Cache duration in minutes (optional, default: 5)
VITE_CACHE_DURATION_MINUTES=5

# Enable/disable API (set to false to use fallback data)
VITE_USE_SHEETS_API=true
