# Google Sheets API Configuration
# Bật/tắt API (true = dùng API, false = dùng fallback data)
VITE_USE_SHEETS_API=false

# URL của Google Apps Script Web App (sau khi deploy)
# Ví dụ: https://script.google.com/macros/s/AKfycb.../exec
VITE_GOOGLE_SHEETS_API_URL=

# ID của Google Spreadsheet (lấy từ URL của sheet)
# Ví dụ: https://docs.google.com/spreadsheets/d/1xFB9YFerq7JXSNCYFI2UrhCgkbOomYIR49kwwgCQkEk/edit
VITE_GOOGLE_SPREADSHEET_ID=1xFB9YFerq7JXSNCYFI2UrhCgkbOomYIR49kwwgCQkEk

# Cache duration (phút)
VITE_CACHE_DURATION_MINUTES=5